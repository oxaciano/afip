<h1>TODOS LOS REGISTROS</h1>
<table class="table table-striped table-responsive table-hover">
  <tr>
    <th scope="col" >ID del Registro</th>
    <th scope="col" >Fecha de comprobante o fecha de oficialización</th>
    <th scope="col" >Tipo de comprobante</th>
    <th scope="col" >Punto de venta</th>
    <th scope="col" >Número de comprobante</th>
    <th scope="col" >Despacho de importación</th>
    <th scope="col" >Código de documento del vendedor</th>
    <th scope="col" >Número de identificación del vendedor</th>
    <th scope="col" >Apellido y nombre o denominación del vendedor</th>
    <th scope="col" >Importe total de la operación</th>
    <th scope="col" >Importe total de conceptos que no integran el
      precio neto gravado</th>
    <th scope="col" >Importe de operaciones exentas</th>
    <th scope="col">Importe de percepciones o pagos a cuenta del
      Impuesto al Valor Agregado</th>
    <th scope="col">Importe de percepciones o pagos a cuenta de
      otros impuestos nacionales</th>
    <th scope="col">Importe de percepciones de Ingresos Brutos</th>
    <th scope="col">Importe de percepciones de Impuestos
      Municipales</th>
    <th scope="col">Importe de Impuestos Internos</th>
    <th scope="col">Código de moneda</th>
    <th scope="col">Tipo de cambio</th>
    <th scope="col">Cantidad de alícuotas de IVA</th>
    <th scope="col">Código de operación</th>
    <th scope="col">Crédito Fiscal Computable</th>
    <th scope="col">Otros Tributos</th>
    <th scope="col">CUIT emisor/corredor</th>
    <th scope="col">Denominación del emisor/corredor</th>
    <th scope="col">IVA comisión</th>
  </tr>
<% all.forEach(element => { %>
  <tr>
    <th scope="row"><%= element.id %> </td>
    <td><%= element.fechaDeComprobante %> </td>
    <td><%= element.tipoDeComprobante  %> </td>
    <td><%= element.puntoDeVenta %> </td>
    <td><%= element.numeroDeComprobante %> </td>
    <td><%= element.despachoDeImportacion %> </td>
    <td><%= element.codigoDelDocumentoDelVendedor %> </td>
    <td><%= element.numeroDeIdentificacionDelVendedor %> </td>
    <td><%= element.apellidoYNombreODenominacionDelVendedor %> </td>
    <td><%= element.importeTotalDeLaOperacion  %> </td>
    <td><%= element.importeTotalQueNoIntegranElPrecioNetoGravado %> </td>
    <td><%= element.importeDeOperacionesExternas %> </td>
    <td><%= element.pagosACuentaDelIVA  %> </td>
    <td><%= element.pagosACuentaDeOtrosImpuestosNacionales %> </td>
    <td><%= element.pagosACuentaDeIngresosBrutos %> </td>
    <td><%= element.pagosACuentaDeImpuestosMunicipales %> </td>
    <td><%= element.importeDeImpuestosInternos  %> </td>
    <td><%= element.codigoDeMoneda %> </td>
    <td><%= element.cantidadDeAlicuotasDeIVA %> </td>
    <td><%= element.codigoDeOperacion %> </td>
    <td><%= element.creditoFiscalComputable %> </td>
    <td><%= element.otrosTributos %> </td>
    <td><%= element.cuitEmisorCorredor %> </td>
    <td><%= element.denominacionEmisorCorredor  %> </td>
    <td><%= element.ivaComision  %> </td>
  </tr>
<% }) %>
</table>